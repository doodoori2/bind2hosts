;
; BIND data file for local loopback interface
;
$TTL  604800
@ IN  SOA a.localhost. root.localhost. (
            2   ; Serial
       604800   ; Refresh
        86400   ; Retry
      2419200   ; Expire
       604800 ) ; Negative Cache TTL
;
@ IN  NS  localhost.
@ IN  A 127.0.0.1
@ IN  AAAA  ::1

SUB1 IN A 10.0.0.1
SUB2 IN A 10.0.0.2
SUB3 IN A 10.0.0.3
SUB4 IN A 10.0.0.4

